    "# Checking and printing duplicates if any\n",
    "\n",
    "import pandas as pd\n",
    "import seaborn as sns\n",
    "\n",
    "df = pd.read_csv('bestsellers.csv')\n",
    "\n",
    "duplicates = df[df.duplicated(['Name'])]\n",
    "\n",
    "if duplicates.empty:\n",
    "    print(\"No duplicates found\")\n",
    "else:\n",
    "    print(\"Duplicates found:\")\n",
    "    print(duplicates)"
   ]
